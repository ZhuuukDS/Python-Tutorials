   "source": [
    "# print out numbers from 1 to n with symbol(s) + showing the number of dividers for these numbers\n",
    "\n",
    "n = int(input())\n",
    "for i in range(1, n + 1):\n",
    "    s = 0\n",
    "    for j in range(1, i + 1):\n",
    "        if i % j == 0:\n",
    "            s += 1\n",
    "    print(i, '+'*s, sep = '') "
   ]
